0001b6 9a2c
0001b7 dfc0      	LCD_WRITE_CHR	
0001b8 953a      	DEC				R19
0001b9 3030      	CPI				R19,		0				
0001ba f7d1      	BRNE			loop
                 
0001bb 982c
0001bc e880
0001bd dfd3      	LCD_WRITE_CMD	0x80							;Set position: col 0 - MAX 94
0001be 982c
0001bf e480
