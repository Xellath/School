                 ;==============================================================================
                 ; Macro for writing a command to the LCD.
                 ; Uses registers:
                 ;	R24				Command data / parameter to 'lcd_write_byte'
                 ;
                 ; Example: Write 0x20 to the LCD
                 ;	LCD_WRITE_CMD	0x20
                 ;==============================================================================
                 	.MACRO			LCD_WRITE_CMD
                 	CBI				PORTB,			4				; clear D/C pin
                 	LDI				R24,			@0				; set command as parameter
                 	RCALL			lcd_write_byte					; write byte to the LCD
                 	.ENDMACRO
                 
                 ;==============================================================================
                 ; Macro for writing a character to the LCD.
                 ; Uses registers:
                 ;	R24				Character / parameter to 'lcd_write_char'
                 ;
                 ; Example: Write 'A' to the LCD
                 ;	LDI				R24,			'A'
                 ;	LCD_WRITE_CHR
                 ; Example: Write current character stored in R24
                 ;	LCD_WRITE_CHR
                 ;==============================================================================
                 	.MACRO			LCD_WRITE_CHR
                 	SBI				PORTB,			4				; set D/C pin
                 	RCALL			lcd_write_char					; write char to the LCD
                 	.ENDMACRO
                 
                 ;==============================================================================
                 ; Macro for writing a string of characters to the LCD.
                 ; Uses registers:
                 ;	ZH:ZL			High and Low part of Z register, for string pointer
                 ;
                 ; Example: Write the string 'my_string', stored in program memory
                 ;	LCD_WRITE_STR	my_string
                 ;==============================================================================
                 	.MACRO			PRINTSTRING
                 	LDI				ZH,				HIGH(@0<<1)
                     LDI				ZL,				LOW(@0<<1)
                 	RCALL			lcd_write_string
                 
                 	.ENDMACRO
                 
                 ;==============================================================================
                 ; Initialize the LCD.
                 ; Uses registers:
                 ;	R24				Used when calling other subroutines.
                 ;==============================================================================
                 lcd_init:
000155 e184      	LDI				R24,			20				; Wait after power-up,
000156 df4d      	RCALL			delay_ms						; let Vcc stabilize!
000157 9a2d      	SBI				PORTB,			5				; Set LCD reset pin HIGH
000158 e08a      	LDI				R24,			10				; Wait before...
000159 df4a      	RCALL			delay_ms						; ...raising...
00015a 9a2e      	SBI				PORTB,			6				; ...CS/SS pin (normal lvl)
00015b 982c
00015c e281
00015d d033      	LCD_WRITE_CMD	0x21							; Function set (H=1)
00015e 982c
00015f e183
000160 d030      	LCD_WRITE_CMD	0x13							; Set bias mode 1:48
000161 982c
000162 ec80
000163 d02d      	LCD_WRITE_CMD	0xC0							; Set Vop (contrast)
000164 982c
000165 e084
000166 d02a      	LCD_WRITE_CMD	0x04							; Set temp coefficient
000167 982c
000168 e280
000169 d027      	LCD_WRITE_CMD	0x20							; Function set (H=0)
00016a 982c
00016b e08c
00016c d024      	LCD_WRITE_CMD	0x0C							; Set display control
00016d 9508      	RET
                 
                 ;==============================================================================
                 ; Write a string of characters to the LCD.
                 ; Uses registers:
                 ;	ZH:ZL			String pointer
                 ;	R3:R2			Temporary storage of string pointer
                 ;	R24				Used for writing characters
                 ;==============================================================================
                 lcd_write_string:
                 iterate_str:
00016e 011f      	MOVW			R3:R2,			ZH:ZL			; Store string pointer
00016f 9184      	LPM				R24,			Z				; Copy character to reg
000170 3080      	CPI				R24,			0				; End of string?
000171 f029      	BREQ			end_of_str						; ...end subroutine!
000172 9a2c
000173 d004      	LCD_WRITE_CHR									; Write char to LCD
000174 01f1      	MOVW			ZH:ZL,			R3:R2			; Restore string pointer
000175 9631      	ADIW			ZH:ZL,			1				; Increment string pointer
000176 cff7      	RJMP			iterate_str						; keep on iterating...
                 end_of_str:
000177 9508      	RET
                 
                 ;==============================================================================
                 ; Write a character to the LCD.
                 ; Uses registers:
                 ;	R1:R0			Initial pointer to character data in program memory,
                 ;					this is where the result of R16xR17 is stored.
                 ;   ZH:ZL			"Moving pointer" to character data
                 ;	R16				Copy of parameter data (character)
                 ;	R17				Character width (without padding)
                 ;	R24				Parameter data/char (input to this subroutine)
                 ;					also used by 'lcd_write_byte'.
                 ;==============================================================================
                 lcd_write_char:
000178 2f08      	MOV				R16,			R24				; copy char to temp reg
000179 5200      	SUBI			R16,			ASCII_OFFSET	; subtract ASCII-OFFSET
00017a e015      	LDI				R17,			CHR_WIDTH		; set multiplier
00017b 9f01      	MUL				R16,			R17				; (ASCII-OFFSET)xCHR_WIDTH
00017c e0f1      	LDI				ZH,				HIGH(chars << 1); Init pointer
00017d e8e2      	LDI				ZL,				LOW(chars << 1)
00017e 0de0      	ADD				ZL,				R0				; Set pointer with result from MUL
00017f 1df1      	ADC				ZH,				R1
000180 9184      	LPM				R24,			Z				; Write 1st segment of char
000181 d00f      	RCALL			lcd_write_byte
000182 9631      	ADIW			ZH:ZL,			1				; Increment pointer
000183 9184      	LPM				R24,			Z				; Write 2nd segment of char
000184 d00c      	RCALL			lcd_write_byte
000185 9631      	ADIW			ZH:ZL,			1				; Increment pointer
000186 9184      	LPM				R24,			Z				; Write 3rd segment of char
000187 d009      	RCALL			lcd_write_byte
000188 9631      	ADIW			ZH:ZL,			1				; Increment pointer
000189 9184      	LPM				R24,			Z				; Write 4th segment of char
00018a d006      	RCALL			lcd_write_byte
00018b 9631      	ADIW			ZH:ZL,			1				; Increment pointer
00018c 9184      	LPM				R24,			Z				; Write 5th segment of char
00018d d003      	RCALL			lcd_write_byte
00018e e080      	LDI				R24,			0x00			; Write 6th segment of char
00018f d001      	RCALL			lcd_write_byte					; (padding)
000190 9508      	RET
                 
                 ;==============================================================================
                 ; Write a byte of data to the LCD, start with the most significant bit.
                 ; Uses registers:
                 ;	R16				Copy of parameter data
                 ;	R17				Bit mask (for filtering MSB)
                 ;	R18				Counter (number of bits written to LCD)
                 ;	R24				Parameter data/byte (input to this subroutine)
                 ;					also used by SET_IO_BIT and CLR_IO_BIT macros.
                 ;==============================================================================
                 lcd_write_byte:
000191 982e      	CBI				PORTB,			6				; Lower CS/SS pin
000192 2f08      	MOV				R16,			R24				; Copy data to temp reg
000193 e020      	LDI				R18,			0				; Reset counter
                 mask_bit:
000194 e810      	LDI				R17,			0x80			; Store bit mask
000195 2310      	AND				R17,			R16				; Mask out MSB
000196 3010      	CPI				R17,			0				; If the MSB is..
000197 f431      	BRNE			bit_is_1						; ..not a 0, branch!
000198 9180 0102
00019a 7b8f
00019b 9380 0102 	CLR_IO_BIT		PORTH,			BIT6_LOW		; Clear data pin
00019d c005      	RJMP			write_to_lcd					; Get ready to write data
                 bit_is_1:
00019e 9180 0102
0001a0 6480
0001a1 9380 0102 	SET_IO_BIT		PORTH,			BIT6_HIGH		; Set data pin
                 write_to_lcd:
0001a3 9180 0102
0001a5 6280
0001a6 9380 0102 	SET_IO_BIT		PORTH,			BIT5_HIGH		; Shift out data..
0001a8 dee1      	RCALL			delay_1_micros					; ..by bit-banging..
0001a9 9180 0102
0001ab 7d8f
0001ac 9380 0102 	CLR_IO_BIT		PORTH,			BIT5_LOW		; ..a clock pulse!
0001ae 0f00      	LSL				R16								; Shift data to the left
0001af 9523      	INC				R18								; Increase counter
0001b0 3028      	CPI				R18,			8				; Shifted all bits?
0001b1 f711      	BRNE			mask_bit						; Move on to next bit
0001b2 9a2e      	SBI				PORTB,			6				; Raise CS/SS pin
0001b3 9508      	RET												; (normal mode)
                 
                 ;==============================================================================
                 ; Clear screen of the LCD
                 ; Uses registers:
                 ;	R16				Copy of parameter data
                 ;	R24				contain ' ' to be written on screen
